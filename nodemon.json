{
  "execMap": {
    "ts": "node --require ts-node/register"
  },
  "env": {
    "NODE_ENV": "development"
  },
  "ext": "ts,tsx,js,jsx,json",
  "ignore": [
    "test",
    "dist",
    "docs",
    "logs",
    ".git",
    "node_modules"
  ],
  "script": "src/index.ts",
  "restartable": "rs",
  "verbose": false,
  "watch": [ "src" ],
  "stdin": false,
  "stdout": true,
  "quiet": true,
  "colours": true
}